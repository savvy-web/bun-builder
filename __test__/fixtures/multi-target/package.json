{
	"name": "@test/multi-target",
	"version": "1.0.0",
	"type": "module",
	"exports": {
		".": "./src/index.ts"
	},
	"publishConfig": {
		"access": "public",
		"targets": [
			{
				"protocol": "npm",
				"registry": "https://registry.npmjs.org/",
				"directory": "dist/npm",
				"access": "public",
				"provenance": true
			},
			{
				"protocol": "npm",
				"registry": "https://npm.pkg.github.com/",
				"directory": "dist/github",
				"access": "public",
				"provenance": true
			}
		]
	}
}
